<div>
  <h1>Want to win?</h1>
  <button name="startButton">Play</button>
  <table>
    <tr>
      <th>Place</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Time (seconds)</th>
    </tr>
    <% @participants.each_with_index do |participant, index| %>
      <tr>
        <td><%=index + 1%></td>
        <td><%=participant.first_name%></td>
        <td><%=participant.last_name%></td>
        <td><%=participant.time%></td>
      </tr>
      <%end%>
  </table>
</div>